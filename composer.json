{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "drupal/address": "^1.9",
        "drupal/addtoany": "^1.14",
        "drupal/admin_toolbar": "^3.0",
        "drupal/advagg": "^4.1",
        "drupal/antibot": "^1.4",
        "drupal/backup_migrate": "^5.0@RC",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/block_class": "^1.3",
        "drupal/block_visibility_groups": "^1.4",
        "drupal/bootstrap_barrio": "^5.1",
        "drupal/bootstrap_sass": "^5.0",
        "drupal/business_rules": "^1.0@beta",
        "drupal/captcha": "^1.1",
        "drupal/chosen": "^3.0",
        "drupal/config_ignore": "^2.3",
        "drupal/config_split": "^2.0@beta",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-project-message": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/ctools": "^3.4",
        "drupal/devel": "^4.1",
        "drupal/editor_advanced_link": "^1.8",
        "drupal/empty_page": "^3.0",
        "drupal/entity_browser": "^2.5",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/env_variables": "^2.1",
        "drupal/environment_indicator": "^4.0",
        "drupal/eu_cookie_compliance": "^1.14",
        "drupal/facets": "^1.6",
        "drupal/facets_pretty_paths": "^1.0",
        "drupal/features": "^3.11",
        "drupal/field_formatter_class": "^1.5",
        "drupal/field_group": "^3.1",
        "drupal/google_tag": "^1.4",
        "drupal/honeypot": "^2.0",
        "drupal/hreflang": "^1.4",
        "drupal/image_delta_formatter": "^1.1",
        "drupal/image_url_formatter": "^1.0",
        "drupal/imagemagick": "^3.1",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/lazy": "^3.6",
        "drupal/legal": "^1.0@RC",
        "drupal/libraries": "^3.0@beta",
        "drupal/link_attributes": "^1.10",
        "drupal/login_emailusername": "^1.2",
        "drupal/login_history": "^1.0",
        "drupal/masquerade": "^2.0@beta",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/menu_per_role": "^1.2",
        "drupal/metatag": "^1.8",
        "drupal/minifyhtml": "^1.10",
        "drupal/nextpre": "^9.0",
        "drupal/page_title_visibility": "^1.3",
        "drupal/paragraphs": "^1.12",
        "drupal/paragraphs_features": "^1.11",
        "drupal/paragraphs_modal_edit": "^1.0",
        "drupal/pathauto": "^1.8",
        "drupal/phone_international": "^2.2",
        "drupal/pwa": "^1.5",
        "drupal/quick_node_clone": "^1.13",
        "drupal/redirect": "^1.6",
        "drupal/redirect_after_login": "^2.7",
        "drupal/robotstxt": "^1.4",
        "drupal/rules": "^3.0@alpha",
        "drupal/schema_metatag": "^2.1",
        "drupal/search_api": "^1.18",
        "drupal/simple_sitemap": "^3.8",
        "drupal/simplenews": "^3.0@alpha",
        "drupal/sitewide_alert": "^1.6",
        "drupal/structure_sync": "^2.0",
        "drupal/swiftmailer": "^2.0",
        "drupal/token": "^1.9",
        "drupal/token_filter": "^1.2",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^2.9",
        "drupal/url_redirect": "^2.62",
        "drupal/user_email_verification": "^1.0",
        "drupal/userprotect": "^1.1",
        "drupal/views_bulk_operations": "^3.10",
        "drupal/views_exposed_filter_blocks": "^1.1",
        "drupal/viewsreference": "^2.0@beta",
        "drupal/webform": "^6.0",
        "drupal/webp": "^1.0@beta",
        "drupal/xmlsitemap": "^1.0",
        "drush/drush": "^10.0.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
